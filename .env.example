# Port tempat aplikasi berjalan
PORT=4000

# API key untuk mengakses endpoint /api/files
# Gunakan nilai random yang kuat dan JANGAN dibagikan ke publik
API_KEY=your_api_key_here

# Durasi default file sebelum kedaluwarsa (dalam menit)
DEFAULT_EXPIRY_MINUTES=180

# Interval pengecekan cleanup otomatis (dalam detik)
CLEANUP_INTERVAL_SECONDS=60
